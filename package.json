{
    "name": "tiktok-to-supabase",
    "version": "1.0.0",
    "description": "A tool to scrape TikTok videos and store data in Supabase",
    "type": "module",
    "main": "api.js",
    "scripts": {
        "start": "node api.js",
        "dev": "node api.js",
        "test": "node test-tiktok-to-supabase.js",
        "test-connection": "node test-supabase-connection.js",
        "upload-existing": "node upload-existing-data.js",
        "setup": "npm install && echo 'Remember to configure your .env file with Supabase credentials'",
        "setup-storage": "node setup-storage.js",
        "api": "node api.js",
        "vercel-build": "node setup-storage.js && echo 'Build completed'",
        "vercel-config": "node get-vercel-config.js"
    },
    "keywords": [
        "tiktok",
        "scraper",
        "supabase",
        "crawler",
        "data-extraction",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@supabase/supabase-js": "^2.39.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "playwright": "^1.42.1"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
